  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    
    <!--Metadata endpoint, 'https://login.microsoftonline.com' for azure, "https://login.chinacloudapi.cn" for Azure China(Mooncake)-->
    <add key="MetadataEndpoint" value="https://login.microsoftonline.com"/>
    <!--Base url of sign-on endpoint: "https://login.windows.net" for azur, and "https://login.chinacloudapi.cn" for Azure China(Mooncake)-->
    <add key="SignOnEndpoint" value="https://login.windows.net"/>
    <!--Microsoft Azure AD Graph API endpoint, "https://graph.windows.net" for azure global, "https://graph.chinacloudapi.cn" for azure China-->
    <add key="GraphAPIEndpoint" value="https://graph.windows.net"/>
    <!--SecurityTokenServiceEndpoint, "https://sts.windows.net" for azure global, 'https://sts.chinacloudapi.cn' for azure China-->
    <add key="SecurityTokenServiceEndpoint" value="https://sts.windows.net"/>

    <add key="ida:ClientID" value="[Enter client ID as obtained from Azure Portal, e.g. 82692da5-a86f-44c9-9d53-2f88d52b478b]"/>
    <add key="ida:Password" value="[Enter app key as obtained from Azure Portal, e.g. rZJJ9bHSi/cYnYwmQFxLYDn/6EfnrnIfKoNzv9NKgbo=]"  />
    <add key="ida:TenantId" value="[Enter TenantId as obtained from Azure Portal ,e.g. f8664fd5-1fbb-4a51-b977-39963dee096a]"/>
    
  </appSettings>
